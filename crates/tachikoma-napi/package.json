{
  "name": "@tachikoma/napi",
  "version": "0.1.0",
  "description": "Native bindings for Tachikoma Forge",
  "main": "index.js",
  "types": "index.d.ts",
  "napi": {
    "name": "tachikoma-napi",
    "triples": {
      "defaults": false,
      "additional": [
        "aarch64-apple-darwin",
        "x86_64-apple-darwin",
        "x86_64-unknown-linux-gnu",
        "x86_64-pc-windows-msvc"
      ]
    }
  },
  "scripts": {
    "build": "napi build --platform --release",
    "build:debug": "napi build --platform",
    "artifacts": "napi artifacts",
    "prepublishOnly": "napi prepublish -t npm"
  },
  "devDependencies": {
    "@napi-rs/cli": "^2.18.0"
  },
  "license": "MIT",
  "files": [
    "index.js",
    "index.d.ts",
    "*.node"
  ]
}
